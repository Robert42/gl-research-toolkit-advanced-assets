#include "uuids.as"

void main(Scene@ scene)
{
  scene.loadSceneLayer(uuids::sponza::sceneLayers::sponza_scenery);
  scene.loadSceneLayer(uuids::sponza::sceneLayers::sponza_building);
  scene.addSceneLayer_debugCamera();
  
  scene.camera[CameraSlot::MAIN_CAMERA] = uuids::sponza::cameras::eyeHeight;
}
